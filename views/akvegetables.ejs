            margin-top: 40px;
          
        }



        .image {
            min-width: 35rem;
            max-width: 35rem;
            height: 17rem;
            transform: translateX(-8rem);
            position: relative;
            box-shadow: .5rem .5rem 3px 1px rgba(0, 0, 0, 0.5);
            border-radius: .8rem;
        }

        .image img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            display: block;
            border-radius: .8rem;
        }

        /* .image::before {
            content: '';
