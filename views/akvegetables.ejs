        .title {
           font-family: 'Original Surfer', cursive;
            font-size: 3.5rem;
            margin-right: 63px;
            text-transform: uppercase;
            color:#0c645e;
        }

        .modal-open {
            padding: 8px 16px;
            background: #ac7b7d;
            background-image:  url("/images/akbg1.jpg");
            color: #fff;
            border-radius: 5px;
            outline: none;
            border: none;
            cursor: pointer;
            margin: 5px 0;
            font-size: 18px;
            font-weight: bold;
            width: 250px;
            position: relative;
            transform: translateX(7rem);
        }

        .modal-open:hover {
            background-image: url("/images/akbg2.jpg")
        }

        .modal {
            position: fixed;
            top: 0;
            right: 0;
            left: 0;
            bottom: 0;
            z-index: 2;
            display: none;
            animation: modal-open .5s;
        }

        .modal-content {
            background-image: url("/images/akmbg.jpg");
            margin-top: 55px;
            width: 80%;
             height: auto;
            margin-bottom: auto;
            margin-left: auto;
            margin-right: auto;
            padding: 8px 24px;
            border-radius: 10px;
            z-index: 4;
        }

        .modal-header {
            font-weight: 500;
            padding: 10px 0;
            font-size: 30px;
            font-weight: 600;
            color: rgb(153, 16, 16);
            
        }

        .modal-header .modal-close {
            float: right;
            font-size: 20px;
           background: none;
           border: none;
            padding: 5px 8px;
            cursor: pointer;
          
        }

        .modal-body {
            color: #08073f;
            font-size: 17px;
            padding: 15px 0;
            font-weight: 800;
        }

        .modal-footer {
            padding: 15px 0;
        }

        .modal-footer .modal-close {
            padding: 8px 16px;
            font-size: 14px;
            border: none;
            outline: none;
            background-image: url('/images/akmbbg.jpg');
            border-radius: 5px;
            color: #fff;
            font-weight: 600;
            cursor: pointer;
              text-transform: uppercase;
        }

        @keyframes modal-open {
            from {
                opacity: 0;
            }

            to {
                opacity: 1;
            }
        }
          

  
    </style>
