<body>
   
    <div class="content">
        <div class="vertical-bar">
            <div class="search-icon">
                <a href="/agriknowledge"><i class="fa fa-chevron-circle-left"></i></a>
                <a href="/"><i class="fa fa-home"></i></a>
    
            </div>
            <div class="social-icons">
            </div>
        </div>
    </div>

    <!-- container 01 -->
    <div class="container"  >
        <div class="image">
            <img src="/images/kvchilli.jpg">
        </div>
        <div class="t">
            <h1 class="title" style="margin-left: 35px;">chilli</h1>
        </div>
        
        <div class="info">
            <button class="modal-open" data-modal="modal1">SOIL & SEASON OF SOWING</button><br>
            <button class="modal-open" data-modal="modal2">SEED TREATMENT</button><br>
            <button class="modal-open" data-modal="modal3">FIELD PREPARATION</button><br>
